<div class="container">
  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <h1 class="page-header">Upload Photo</h1>
        <%= bootstrap_form_for @image, url: admin_images_path do |f| %>

          <%= f.text_field :name %>
          <%= f.file_field :image %>

          <%= f.submit "Save", :id => 'save', class: "btn btn-success" %>
          <%= link_to "Dashboard", admin_path(@business.slug), class: "btn btn-default" %>
        <% end %>
      </div>
  </div>
</div>
