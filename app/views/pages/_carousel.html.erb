<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
    <li data-target="#myCarousel" data-slide-to="3"></li>
  </ol>
  <div class="carousel-inner" style="  height: 739px;"
>
    <div class="item active">
      <img src="https://s3-us-west-2.amazonaws.com/turing-gallery/beach-street.png"
       alt="First slide">
        <div class="container">
          <div class="carousel-caption">
            <h1><%= @home.tagline1 %></h1>
            <p><%= @home.caption1 %></p>
          </div>
        </div>
      </div>
      <div class="item">
        <img src="https://s3-us-west-2.amazonaws.com/turing-gallery/sailboat.png" alt="Second slide">
        <div class="container">
          <div class="carousel-caption">
            <h1><%= @home.tagline2 %></h1>
            <p><%= @home.caption2 %></p>
          </div>
        </div>
      </div>
      <div class="item">
        <img src="https://s3-us-west-2.amazonaws.com/turing-gallery/bubbles.png" alt="Third slide">
        <div class="container">
          <div class="carousel-caption">
            <h1><%= @home.tagline3 %></h1>
            <p><%= @home.caption3 %></p>
          </div>
        </div>
      </div>
      <div class="item">
        <img src="https://s3-us-west-2.amazonaws.com/turing-gallery/sun-through-clouds.png" alt="Third slide">
        <div class="container">
          <div class="carousel-caption">
            <h1><%= @home.tagline4 %></h1>
            <p><%= @home.caption4 %></p>
          </div>
        </div>
    </div>
  </div><!-- /.carousel -->
</div>
